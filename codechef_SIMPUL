//codechef problem
//Problem: SIMPUL Contest: COVO2020 



#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n, q;
    cin >> n;
    if (n >= 1)
    {
        vector<pair<int, int>> pairs;
        int l, r, h, count = 0;
        while (n > 0)
        {
            cin >> l >> r;
            pairs.push_back(make_pair(l, r));
            n--;
        }
        cin >> q;
        if (q >= 1)
        {
            while (q > 0)
            {
                count = 0;
                cin >> l >> r >> h;
                for (int i = 0; i < pairs.size(); i++)
                {
                    if (l <= pairs[i].first && pairs[i].first <= r && pairs[i].second == h)
                    {
                        pairs.erase(pairs.begin() + i);
                        i--;
                        count++;
                    }
                }
                cout << count << endl;
                q--;
            }
        }
    }
    return 0;
}
