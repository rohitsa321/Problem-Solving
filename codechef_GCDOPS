//codechef problem

#include <bits/stdc++.h>
using namespace std;
int a1[10000];
int b1[10000];
int gcd(int a, int b)
{
   // Everything divides 0
   if (a == 0)
      return b;
   if (b == 0)
      return a;

   // base case
   if (a == b)
      return a;

   // a is greater
   if (a > b)
      return gcd(a - b, b);
   return gcd(a, b - a);
}
bool check(int n, int end)
{
   for (int i = 0; i < n; ++i)
   {
      if (i == end)
      {
         continue;
      }
      int temp = gcd(a1[i], a1[end]);

      if (temp == b1[i] && temp == b1[end])
      {
         return true;
      }
   }
   return false;
}

int main()
{
   ios_base::sync_with_stdio(false);
   cin.tie(nullptr);

   int T, n;
   cin >> T;

   while (T > 0)
   {
      cin >> n;
      int i;
      bool ans = true;
      for (i = 0; i < n; i++)
      {
         cin >> b1[i];
         a1[i] = i + 1;
      }
      for (i = 0; i < n; i++)
      {
         if (a1[i] != b1[i])
         {
            if (check(n, i))
            {
               ans = true;
            }
            else
            {
               ans = false;
            }
         }
      }
      if (ans)
      {
         cout << "YES" << endl;
      }
      else
      {
         cout << "NO" << endl;
      }
      T--;
   }
   return 0;
}
